<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>JSTL if</title>
</head>
<body>
	<!-- JSTL if 조건 -->
	<c:if test="true">
		<!-- test 조건이 true인 경우에만 실행 -->
		true 이므로 실행되어야 함.	
	</c:if>
</body>
</html>